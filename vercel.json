{
  "version": 2,
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "dist/server/index.js": {
      "runtime": "@vercel/node",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
